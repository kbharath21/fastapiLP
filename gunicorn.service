[Unit]
Description=demo fastapi application
After=network.target

[Service]
User=bharath
Group=bharath
WorkingDirectory=/home/bharath/app/fastapiLP/
Environment="PATH=/home/bharath/app/venv/bin"
EnvironmentFile=/home/bharath/.env
ExecStart=/home/bharath/app/venv/bin/gunicorn -w 4 -k uvicorn.workers.UvicornWorker app.main:app --bind 0.0.0.0:8000

[Install]
WantedBy=multi-user.target